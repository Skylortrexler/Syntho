<Defs>  
  <ThingDef ParentName="AnimalThingBase">
    <defName>EnargoGazelle</defName>
    <label>e-gazelle</label>
    <description>a genetically modified gazelle designed to be both a friend and source of food, when in need. Drastically reduced lifespan.</description>
    <statBases>
      <MoveSpeed>6.2</MoveSpeed>
      <ComfyTemperatureMin>-10</ComfyTemperatureMin>
      <MarketValue>220</MarketValue>
    </statBases>
	<comps>
      <li Class="CompProperties_Milkable">
        <milkDef>MiniEnargo</milkDef>
        <milkIntervalDays>1</milkIntervalDays>
        <milkAmount>4</milkAmount>
		<milkFemaleOnly>false</milkFemaleOnly>
      </li>
    </comps>
    <tools>
      <li>
        <label>left hoof</label>
        <capacities>
          <li>Blunt</li>
          <li>Poke</li>
        </capacities>
        <power>6</power>
        <cooldownTime>1.65</cooldownTime>
        <linkedBodyPartsGroup>FrontLeftLeg</linkedBodyPartsGroup>
      </li>
      <li>
        <label>right hoof</label>
        <capacities>
          <li>Blunt</li>
          <li>Poke</li>
        </capacities>
        <power>6</power>
        <cooldownTime>1.65</cooldownTime>
        <linkedBodyPartsGroup>FrontRightLeg</linkedBodyPartsGroup>
      </li>
      <li>
        <capacities>
          <li>Bite</li>
        </capacities>
        <power>4</power>
        <cooldownTime>1.65</cooldownTime>
        <linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>2</power>
        <cooldownTime>1.65</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <commonality>0.2</commonality>
      </li>
    </tools>
    <race>
      <body>QuadrupedAnimalWithHooves</body>
      <herdAnimal>true</herdAnimal>
      <baseBodySize>0.7</baseBodySize>
      <baseHungerRate>0.35</baseHungerRate>
      <baseHealthScale>0.7</baseHealthScale>
      <foodType>VegetarianRoughAnimal</foodType>
      <leatherColor>(214,134,59)</leatherColor>
      <leatherInsulation>0.75</leatherInsulation>
      <wildness>0.75</wildness>
      <gestationPeriodDays>15</gestationPeriodDays>
      <lifeExpectancy>3</lifeExpectancy>
      <lifeStageAges>
        <li>
          <def>AnimalAdult</def>
          <minAge>0.0</minAge>
          <soundWounded>Pawn_Deer_Wounded</soundWounded>
          <soundDeath>Pawn_Deer_Death</soundDeath>
          <soundCall>Pawn_Deer_Call</soundCall>
          <soundAngry>Pawn_Deer_Angry</soundAngry>
        </li>
      </lifeStageAges>
      <soundMeleeHitPawn>Pawn_Melee_BigBash_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_BigBash_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_BigBash_Miss</soundMeleeMiss>
    </race>
    <tradeTags>
      <li>StandardAnimal</li>
	  <li>BadassAnimal</li>
    </tradeTags>
  </ThingDef>

  <PawnKindDef ParentName="AnimalKindBase">
    <defName>EnargoGazelleMag</defName>
    <label>e-gazelle</label>
    <race>EnargoGazelle</race>
    <combatPower>40</combatPower>
    <wildSpawn_spawnWild>true</wildSpawn_spawnWild>
    <wildSpawn_EcoSystemWeight>1</wildSpawn_EcoSystemWeight>
    <wildSpawn_GroupSizeRange>
      <min>2</min>
      <max>5</max>
    </wildSpawn_GroupSizeRange>
    <lifeStages>
      <li>
        <bodyGraphicData>
          <texPath>SynthoAnimals/GazelleMag</texPath>
          <drawSize>1.65</drawSize>
          <shadowData>
            <volume>(0.4, 0.4, 0.4)</volume>
            <offset>(0,0,-0.2)</offset>
          </shadowData>
        </bodyGraphicData>
        <dessicatedBodyGraphicData>
          <texPath>Things/Pawn/Animal/Dessicated/CritterDessicatedMedium</texPath>
          <drawSize>1.65</drawSize>
        </dessicatedBodyGraphicData>
      </li>
    </lifeStages>
  </PawnKindDef>
