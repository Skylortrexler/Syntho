<?xml version="1.0" encoding="utf-8" ?>
<Defs>



	<ThingDef Name="ResourceBaseEnargo" Abstract="True">
		<thingClass>ThingWithComps</thingClass>
		<label>Enargo</label>
		<category>Item</category>
		<resourceReadoutPriority>Middle</resourceReadoutPriority>
		<useHitPoints>true</useHitPoints>
		<selectable>true</selectable>
		<altitudeLayer>Item</altitudeLayer>
		<stackLimit>75</stackLimit>
		<comps>
			<li Class="CompProperties_Forbiddable"/>
		</comps>
		<alwaysHaulable>true</alwaysHaulable>
		<drawGUIOverlay>true</drawGUIOverlay>
		<rotatable>false</rotatable>
		<pathCost>15</pathCost>
	</ThingDef>



	<ThingDef Abstract="True" Name="PlantFoodEnargoBase" ParentName="ResourceBaseEnargo">
		<graphicData>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<socialPropernessMatters>true</socialPropernessMatters>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<Flammability>0.5</Flammability>
			<DeteriorationRate>10</DeteriorationRate>
			<Mass>0.03</Mass>
		</statBases>
		<tickerType>Rare</tickerType>
		<ingestible>
			<preferability>RawBad</preferability>
			<ingestEffect>EatVegetarian</ingestEffect>
			<ingestSound>RawVegetable_Eat</ingestSound>
		</ingestible>
		<thingCategories>
			<li>PlantFoodRaw</li>
		</thingCategories>
		<comps>
			<li Class="CompProperties_FoodPoisoningChance">
				<humanlikeOnly>True</humanlikeOnly>
			</li>
		</comps>
	</ThingDef>



	<ThingDef ParentName="PlantFoodEnargoBase">
		<defName>RawEnargo</defName>
		<label>Enargo</label>
		<category>Item</category>
		<description>A small bio-cell plant that Syntho use. </description>
		<graphicData>
			<texPath>Enargo/RawEnargo/Enargo</texPath>
		</graphicData>
		<statBases>
			<MarketValue>2.5</MarketValue>
		</statBases>
		<rotatable>false</rotatable>
		<ingestible>
			<preferability>RawBad</preferability>
			<nutrition>0.2</nutrition>
			<foodType>VegetableOrFruit</foodType>
			<ingestEffect>EatVegetarian</ingestEffect>
			<ingestSound>RawVegetable_Eat</ingestSound>
		</ingestible>
	</ThingDef>

		<ThingDef ParentName="PlantFoodEnargoBase">
		<defName>MiniEnargo</defName>
		<label>Mini-Enargo</label>
		<category>Item</category>
		<description>An even smaller Enargo cell created by an E-Rat.</description>
		<graphicData>
			<texPath>Enargo/RawEnargo/MiniEnargo</texPath>
		</graphicData>
		<statBases>
			<MarketValue>1.3</MarketValue>
		</statBases>
		<rotatable>false</rotatable>
		<ingestible>
			<preferability>MealSimple</preferability>
			<nutrition>0.05</nutrition>
			<foodType>VegetableOrFruit</foodType>
			<ingestEffect>EatVegetarian</ingestEffect>
			<ingestSound>RawVegetable_Eat</ingestSound>
		</ingestible>
	</ThingDef>

		<ThingDef ParentName="PlantFoodEnargoBase">
		<defName>LiquidEnargo</defName>
		<label>Liquid Enargo</label>
		<category>Item</category>
		<description>A liquidfied enargo solution. High calories, low fat.</description>
		<graphicData>
			<texPath>Enargo/RawEnargo/LiquidEnargo</texPath>
		</graphicData>
		<statBases>
			<MarketValue>1.25</MarketValue>
		</statBases>
		<rotatable>false</rotatable>
		<ingestible>
			<preferability>MealSimple</preferability>
			<nutrition>0.1</nutrition>
			<foodType>VegetableOrFruit</foodType>
			<ingestEffect>EatVegetarian</ingestEffect>
			<ingestSound>RawVegetable_Eat</ingestSound>
		</ingestible>
	</ThingDef>
</Defs>
